{
  "printWidth": 100,
  "tabWidth": 2,
  "trailingComma": "none",
  "singleQuote": true,
  "semi": false,
  "endOfLine": "lf",
  "importOrder": [
    "@/app/styles/global.css",
    "^react(-.*|/.*)?$",
    "<THIRD_PARTY_MODULES>",
    "^(?:@/)/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["prettier-plugin-tailwindcss", "@trivago/prettier-plugin-sort-imports"]
}
